generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SolarComponent {
  id               String @id @default(uuid()) @db.Uuid
  gtin             String
  name             String
  segmentType      String
  group            String
  height           Int
  width            Int
  depth            Int
  grossWeight      Int
  netWeight        Int

  // @@map("solarComponents")
}